SELECT A1.*
	, CASE
		WHEN A1.TOT_ROW % 5 = 0 
			THEN A1.TOT_ROW / 5
		ELSE (A1.TOT_ROW / 5)+1
		END AS TOT_PAGE
FROM(
SELECT A.NO
	   , A.TIKER
	   , A.SCR
	   , A.TP0
	   , A.SIDE
	   , A.KST_DATE
	   , A.INCOME_PER
	   , A.PROFIT_KRW
	   , A.ACCOUNT_KRW
	   , ROW_NUMBER() OVER(ORDER BY A.KST_DATE DESC ) AS DESC_NO
	   , COUNT(A.NO) OVER() TOT_ROW
FROM TB_COIN_INCOME A
	WHERE DATE(A.KST_DATE) BETWEEN '2025-04-18' AND '2025-04-27'
)A1
LIMIT 5*(1-1), 5

